{
    "name": "dconco/slides-status",
    "description": "PhpSlides Status Response in handling API",
    "homepage": "https://github.com/dconco/slides-status",
    "type": "library",
    "keywords": ["slides-status", "status", "api", "slides"],
    "authors": [
        {
            "name": "Dave Conco",
            "email": "concodave@gmail.com",
            "homepage": "https://dconco.github.io"
        }
    ],
    "version": "0.0.1",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "PhpSlides\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpSlides\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        },
        "preferred-install": "dist"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    },
    "scripts": {
        "test": "phpunit",
        "test-debug": "phpunit --debug"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
